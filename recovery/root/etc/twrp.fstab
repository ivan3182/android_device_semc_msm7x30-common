# <mount point> <type>       <file system>            <flags>

/cache          yaffs2      cache
/system         yaffs2      system
/data           ext4        /dev/block/mmcblk0p2      flags=display="Data (ext4)";removable
/data           f2fs        /dev/block/mmcblk0p2      flags=display="Data (f2fs)";removable
/sdcard         auto        /dev/block/mmcblk0p1      flags=display="Micro SDcard";storage;removable
/usb-otg        auto        /dev/block/sda1           flags=display="Micro SDcard";storage;removable
